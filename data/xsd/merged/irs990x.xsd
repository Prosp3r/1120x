					<xsd:documentation>
						<Description>Qualified conservation contributions - other</Description>
						<LineNumber>Part I Line 14</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Real Estate - Residential -->
			<xsd:element name="RealEstateResidentialGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Real estate - residential</Description>
						<LineNumber>Part I Line 15</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Real Estate - Commercial -->
			<xsd:element name="RealEstateCommercialGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Real estate - commercial</Description>
						<LineNumber>Part I Line 16</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Real Estate - Other -->
			<xsd:element name="RealEstateOtherGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Real estate - other</Description>
						<LineNumber>Part I Line 17</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Collectibles -->
			<xsd:element name="CollectiblesGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collectibles</Description>
						<LineNumber>Part I Line 18</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Food Inventory -->
			<xsd:element name="FoodInventoryGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Food inventory</Description>
						<LineNumber>Part I Line 19</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Drugs and Medical Supplies -->
			<xsd:element name="DrugsAndMedicalSuppliesGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Drugs and medical supplies</Description>
						<LineNumber>Part I Line 20</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxidermy -->
			<xsd:element name="TaxidermyGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxidermy</Description>
						<LineNumber>Part I Line 21</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Historical Artifacts -->
			<xsd:element name="HistoricalArtifactsGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Historical artifacts</Description>
						<LineNumber>Part I Line 22</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Scientific Specimens -->
			<xsd:element name="ScientificSpecimensGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Scientific specimens</Description>
						<LineNumber>Part I Line 23</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Archaeological Artifacts -->
			<xsd:element name="ArchaeologicalArtifactsGrp" type="Form990SchMGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Archaeological artifacts</Description>
						<LineNumber>Part I Line 24</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Non-Cash Contributions Repeating Table -->
			<xsd:element name="OtherNonCashContriTableGrp" type="Form990SchMGroup3Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other non-cash contributions</Description>
						<LineNumber>Part I Lines 25-28</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Number of 8283s Received -->
			<xsd:element name="Form8283ReceivedCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of 8283s received</Description>
						<LineNumber>Part I Line 29</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Any Property that Must be Held? -->
			<xsd:element name="AnyPropertyThatMustBeHeldInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Any property that must be held?</Description>
						<LineNumber>Part I Line 30a</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Review Process Reference Unusual Noncash Gifts? -->
			<xsd:element name="ReviewProcessUnusualNCGiftsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Review process reference unusual noncash gifts?</Description>
						<LineNumber>Part I Line 31</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Third Parties Used? -->
			<xsd:element name="ThirdPartiesUsedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Third parties used?</Description>
						<LineNumber>Part I Line 32a</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part II - Repeating Explanation Table -->
			<!-- Explanation Table -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Explanation repeating group</Description>
						<LineNumber>Part II</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
					
						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
									<LineNumber>Part II</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Reference Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference explanation</Description>
									<LineNumber>Part II</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- =========================================================================== -->
	<!-- ============= Local types, types for repeating groups etc. ================ -->
	<!-- =========================================================================== -->

	<!-- Group for Part I, Non-Cash Contributions Checkbox Column (a) Table -->
	<xsd:group name="Form990SchMGroup1">
		<xsd:sequence>

			<!-- Checkbox for Non-Cash Contribtuion  -->
			<xsd:element name="NonCashCheckboxInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Checkbox for lines on Part I</Description>
						<LineNumber>Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:group>

	<!-- Group for Part I, Non-Cash Contributions Column (b) Table -->
	<xsd:group name="Form990SchMGroup2">
		<xsd:sequence>

			<!-- Number of Contributions -->
			<xsd:element name="ContributionCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of contributions</Description>
						<LineNumber>Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:group>

	<!-- Group for Part I, Non-Cash Contributions Columns (c) and (d) Table -->
	<xsd:group name="Form990SchMGroup3">
		<xsd:sequence>

			<!-- Revenues Reported On F990, Part VIII, Line 1G -->
			<xsd:element name="NoncashContributionsRptF990Amt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Revenues reported on F990, Pt VIII, line 1g</Description>
						<LineNumber>Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Method of Determining Revenues -->
			<xsd:element name="MethodOfDeterminingRevenuesTxt" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Method of determining revenues</Description>
						<LineNumber>Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:group>

	<!-- Type for Part I, Non-Cash Contributions Checkbox Plus Columns (b) through (d) Table -->
	<xsd:complexType name="Form990SchMGroup1Type">
		<xsd:sequence>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup1"/>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup2"/>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup3"/>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Type for Part I, Non-Cash Contributions Checkbox Plus Columns (c) and (d) Table -->
	<xsd:complexType name="Form990SchMGroup2Type">
		<xsd:sequence>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup1"/>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup3"/>

		</xsd:sequence>
	</xsd:complexType>

	<!-- Type for Part I, Non-Cash Contributions Other Repeating Group -->
	<!-- Description Plus Columns (a) through (d) -->	
	<xsd:complexType name="Form990SchMGroup3Type">
		<xsd:sequence>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup1"/>

			<!-- Description -->
			<xsd:element name="Desc" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description</Description>
						<LineNumber>Lines 25 - 28</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup2"/>

			<!-- Other Repeating Elements Joined -->
			<xsd:group ref="Form990SchMGroup3"/>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule N ================ -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleN">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule N</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleNType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleN">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule N (Form 990) Liquidation, Termination, Dissolution or Significant Disposition of Assets -->

	<xsd:complexType name="IRS990ScheduleNType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule N</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>   

			<!-- Part I, Liquidation, Termination or Dissolution -->
			<xsd:element name="LiquidationOfAssetsTableGrp" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete Part I if the organization ceased its operations and any remaining activities are for the purpose of dissolving, paying debts, or distributing any remaining assets</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="LiquidationOfAssetsDetail" type="Form990SchNGroup1Type" minOccurs="0" maxOccurs="unbounded"/>
					</xsd:sequence>
						<xsd:attribute name="referenceDocumentId" type="IdListType"/>
						<xsd:attribute name="referenceDocumentName" type="StringType" fixed="BinaryAttachment"/>
				</xsd:complexType>
			</xsd:element>

			<!-- Line 2 Questions, Did or will any officer, director, trustee, or key employee of the organization: -->
			<!-- Director of Successor? -->
			<xsd:element name="DirectorOfSuccessorInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become a director or trustee of a successor or transferee organization?</Description>
						<LineNumber>Part I Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Employee of Successor? -->
			<xsd:element name="EmployeeOfSuccessorInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become an employee of, or independent contractor for, a successor or transferee organization?</Description>
						<LineNumber>Part I Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Owner of Successor? -->
			<xsd:element name="OwnerOfSuccessorInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become a direct or indirect owner of a successor or transferee organization?</Description>
						<LineNumber>Part I Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Receive Compensation? -->
			<xsd:element name="ReceiveCompensationInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Receive, or become entitled to, compensation or other similar payments as a result of the organization's liquidation, termination, or dissolution?</Description>
						<LineNumber>Part I Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Assets Distributed? -->
			<xsd:element name="AssetsDistributedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization distribute its assets in accordance with its governing instruments?</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Required to Notify AG? -->
			<xsd:element name="RequiredToNotifyAGInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is the organization required to notify the attorney general or other appropriate state official of its intent to dissolve?</Description>
						<LineNumber>Part I Line 4a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- AG Notified? -->
			<xsd:element name="AttorneyGeneralNotifiedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If "Yes," did the organization provide such notice?</Description>
						<LineNumber>Part I Line 4b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Liabilities Paid? -->
			<xsd:element name="LiabilitiesPaidInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization discharge or pay all liabilities in accordance with state laws?</Description>
						<LineNumber>Part I Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Bonds Outstanding? -->
			<xsd:element name="BondsOutstandingInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization have any tax-exempt bonds outstanding during the year?</Description>
						<LineNumber>Part I Line 6a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Bond Liabilities Discharged? -->
			<xsd:element name="BondLiabilitiesDischargedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization discharge or defease tax-exempt bond liabilities in accordance with the Internal Revenue Code and state laws?</Description>
						<LineNumber>Part I Line 6b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part II, Sale, Exchange, Disposition or Other Transfer of more than 25% of the Organization's Assets -->
			<xsd:element name="DispositionOfAssetsDetail" type="Form990SchNGroup1Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete Part II if the organization had a disposition of more than 25% of its assets</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part II, Line 2 Questions: Did or will any officer, director, trustee, or key employee of the organization: -->
			<!-- Director of Successor? -->
			<xsd:element name="DirectorOfSuccessor2Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become a director or trustee of a successor or transferee organization?</Description>
						<LineNumber>Part II Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Employee of Successor? -->
			<xsd:element name="EmployeeOfSuccessor2Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become an employee of, or independent contractor for, a successor or transferee organization?</Description>
						<LineNumber>Part II Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Owner of Successor? -->
			<xsd:element name="OwnerOfSuccessor2Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Become a direct or indirect owner of a successor or transferee organization?</Description>
						<LineNumber>Part II Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Receive Compensation? -->
			<xsd:element name="ReceiveCompensation2Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Receive, or become entitled to, compensation or other similar payments as a result of the organization's significant disposition of assets?</Description>
						<LineNumber>Part II Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part III, Explanations -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="5">		
				<xsd:annotation>
					<xsd:documentation>Part III contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>		
					<xsd:sequence>
						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
								<LineNumber>Part III</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Reference Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference explanation</Description>
								<LineNumber>Part III</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>	
				</xsd:complexType>		
			</xsd:element>		

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- =========================================================================== -->
	<!-- ============= Local types, types for repeating groups etc. ================ -->
	<!-- =========================================================================== -->
	<!-- Type for Part I and Part II, Columns (a) through (g) -->
	<xsd:complexType name="Form990SchNGroup1Type">
		<xsd:sequence>
	
			<!-- Description of Asset -->
			<xsd:element name="AssetsDistriOrExpnssPaidDesc" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of asset(s) distributed or transactional expenses paid</Description>
						<LineNumber>Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Date of Distribution -->
			<xsd:element name="DistributionDt" type="DateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Date of distribution</Description>
						<LineNumber>Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- FMV of Asset -->
			<xsd:element name="FairMarketValueOfAssetAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Fair market value of asset(s) distributed or amount of transactional expenses</Description>
						<LineNumber>Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Method of FMV Determination -->
			<xsd:element name="MethodOfFMVDeterminationTxt" type="ShortExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Method of determining FMV for asset(s) distributed or transactional expenses</Description>
						<LineNumber>Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- EIN -->
			<xsd:element name="EIN" type="EINType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>EIN of recipient (if tax-exempt)</Description>
						<LineNumber>Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Name of Recipient -->
			<xsd:choice minOccurs="0">
				<xsd:element name="PersonNm" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name - Person</Description>
							<LineNumber>Column (f)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				
				<xsd:element name="BusinessName" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Name - Business</Description>
							<LineNumber>Column (f)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Address of Recipient -->
			<xsd:choice minOccurs="0">
				<xsd:element name="USAddress" type="USAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Address - US</Description>
							<LineNumber>Column (f)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="ForeignAddress" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Address - Foreign</Description>
							<LineNumber>Column (f)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- IRC Section or Type of Entity -->
			<xsd:element name="IRCSectionTxt" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>IRC code section of recipient(s) (if tax-exempt) or type of entity </Description>
						<LineNumber>Column (g)</LineNumber>
