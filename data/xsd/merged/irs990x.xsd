						<xsd:element name="PermitReportToCreditAgencyInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Report to credit agency permitted</Description>
									<LineNumber>Part V Section B Line 18a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Selling Debt Permitted -->
						<xsd:element name="PermitSellingDebtInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Selling debt permitted</Description>
									<LineNumber>Part V Section B Line 18b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Actions That Required a Legal or Judicial Process -->
						<xsd:element name="PermitLegalJudicialProcessInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Actions that required a legal or judicial process</Description>
									<LineNumber>Part V Section B Line 18c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Actions Permitted -->
						<xsd:element name="PermitOtherActionsInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other actions permitted</Description>
									<LineNumber>Part V Section B Line 18d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- None of These Actions or Similar Actions Permitted -->
						<xsd:element name="PermitNoActionsInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>None of these actions or similar actions permitted</Description>
									<LineNumber>Part V Section B Line 18e</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Authorize Third-Party to Engage in Collection Activities? -->
						<xsd:element name="CollectionActivitiesInd" type="BooleanType" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Authorize third-party to engage in collection activities?</Description>
									<LineNumber>Part V Section B Line 19</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Credit Agency Engaged -->
						<xsd:element name="ReportingToCreditAgencyInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Credit agency engaged</Description>
									<LineNumber>Part V Section B Line 19a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Selling Debt Engaged -->
						<xsd:element name="EngagedSellingDebtInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Selling debt engaged</Description>
									<LineNumber>Part V Section B Line 19b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Engaged Actions That Required a Legal or Judicial Process -->
						<xsd:element name="EngagedLegalJudicialProcessInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Engaged actions that required a legal or judicial process</Description>
									<LineNumber>Part V Section B Line 19c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Actions Engaged -->
						<xsd:element name="OtherActionsInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other actions engaged</Description>
									<LineNumber>Part V Section B Line 19d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Notified Upon Admission -->
						<xsd:element name="FAPNotifiedUponAdmissionInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Notified upon admission</Description>
									<LineNumber>Part V Section B Line 20a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Notified Prior to Discharge -->
						<xsd:element name="FAPNotifiedBeforeDischargeInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Notified prior to discharge</Description>
									<LineNumber>Part V Section B Line 20b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Notified Patients in Communications -->
						<xsd:element name="FAPNotifiedAllPatientsInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Notified patients in communications</Description>
									<LineNumber>Part V Section B Line 20c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Documented Determination -->
						<xsd:element name="DocumentedEligDeterminationInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Documented determination</Description>
									<LineNumber>Part V Section B Line 20d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Action Taken -->
						<xsd:element name="OtherActionsTakenInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other action taken</Description>
									<LineNumber>Part V Section B Line 20e</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- None of These Efforts Were Made -->
						<xsd:element name="NoneMadeInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>None of these efforts were made</Description>
									<LineNumber>Part V Section B Line 20f</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Non-Discriminatory Emergency Medical Care Policy? -->
						<xsd:element name="NondisEmergencyCarePolicyInd" type="BooleanType" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Non-discriminatory emergency medical care policy?</Description>
									<LineNumber>Part V Section B Line 21</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- No Emergency Care -->
						<xsd:element name="NoEmergencyCareInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>No emergency care</Description>
									<LineNumber>Part V Section B Line 21a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- No Emergency Policy  -->
						<xsd:element name="NoEmergencyCarePolicyInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>No emergency policy </Description>
									<LineNumber>Part V Section B Line 21b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Limits Emergency Care -->
						<xsd:element name="EmergencyCareLimitedInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Limits emergency care</Description>
									<LineNumber>Part V Section B Line 21c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Reason -->
						<xsd:element name="OtherReasonInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other reason</Description>
									<LineNumber>Part V Section B Line 21d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Charged Amount in Excess of Lowest Negotiated Rates? -->
						<xsd:element name="LowestNegotiatedRatesInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Charged amount in excess of lowest negotiated rates?</Description>
									<LineNumber>Part V Section B Line 22a</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Charged Amount in Excess of Average Negotiated Rates? -->
						<xsd:element name="AverageNegotiatedRatesInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Charged amount in excess of average negotiated rates?</Description>
									<LineNumber>Part V Section B Line 22b</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Charged More Than Medicare Rate? -->
						<xsd:element name="MedicareRatesInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Charged more than medicare rate?</Description>
									<LineNumber>Part V Section B Line 22c</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Used Other Method -->
						<xsd:element name="OtherMethodUsedInd" type="CheckboxType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Used other method</Description>
									<LineNumber>Part V Section B Line 22d</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>


						<!-- Charged More Than Amount Generally Billed? -->
						<xsd:element name="AmountsGenerallyBilledInd" type="BooleanType" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Charged more than amount generally billed?</Description>
									<LineNumber>Part V Section B Line 23</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Gross Charges in Billing? -->
						<xsd:element name="GrossChargesInd" type="BooleanType" minOccurs="1">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Gross charges in billing?</Description>
									<LineNumber>Part V Section B Line 24</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>

			</xsd:element>

			<!-- Part V Section C - Supplemental Information for Part V, Section B -->
			<xsd:element name="SupplementalInformationGrp" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part V Section C, Supplemental Information for Part V Section B</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
									<LineNumber>Part V</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Reference Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference explanation</Description>
									<LineNumber>Part V</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>

			</xsd:element>


			<!-- Part V Section D - Other Health Care Facilities -->

			<!-- Number of Other Facilities -->
			<xsd:element name="FacilityNum" type="Count2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of other facilities</Description>
						<LineNumber>Part V Section D</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="OthHlthCareFcltsNotHospitalGrp" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part V section D, other health care facilities</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Repeating Group -->
						<xsd:element name="OthHlthCareFcltsGrp" minOccurs="1" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Part V Section D repeating group</Description>
								</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>

									<!-- Name -->
									<xsd:element name="BusinessName" type="BusinessNameType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Name, business</Description>
												<LineNumber>Part V Section D</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

									<!-- Address -->
									<xsd:element name="USAddress" type="USAddressType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Address - US</Description>
												<LineNumber>Part V Section D</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

									<!-- Facility Type -->
									<xsd:element name="FacilityTxt" type="LineExplanationType">
										<xsd:annotation>
											<xsd:documentation>
												<Description>Facility type</Description>
												<LineNumber>Part V Section D</LineNumber>
											</xsd:documentation>
										</xsd:annotation>
									</xsd:element>

								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Part VI Supplemental Information -->
			<!-- Repeating Group -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Part VI contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
									<LineNumber>Part VI</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number explanation</Description>
									<LineNumber>Part VI</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<!-- =========================================================================== -->
