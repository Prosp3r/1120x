						<LineNumber>Part II and III Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Current Tax Year Minus Three Years -->
			<xsd:element name="CurrentTaxYearMinus3YearsAmt" type="USAmountType" minOccurs="0">
