				</xsd:annotation>
			</xsd:element>

			<!-- Description of How Gift is Held -->
			<xsd:element name="HowGiftIsHeldDesc" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Description of how gift is held</Description>
						<LineNumber>Part III Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Transferee Name -->
			<xsd:choice minOccurs="0">
				<xsd:element name="TransfereeNameBusiness" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Transferee name - Business</Description>
							<LineNumber>Part III Column (e)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="TransfereeNameIndividual" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Transferee name - Individual</Description>
							<LineNumber>Part III Column (e)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>			

			<!-- Transferee Address -->
			<xsd:choice minOccurs="0">			
				<xsd:element name="TransfereeUSAddress" type="USAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Transferee address - US</Description>
							<LineNumber>Part III Column (e)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="TransfereeForeignAddress" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Transferee address - Foreign</Description>
							<LineNumber>Part III Column (e)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>						

			<!-- Relationship of Transferor to Transferee -->
			<xsd:element name="RlnOfTransferorToTransfereeTxt" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Relationship of transferor to transferee</Description>
						<LineNumber>Part III Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule C ================ -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleC">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule C</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleCType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleC">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. to avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule C (Form 990) Political Campaign and Lobbying Activities -->

	<xsd:complexType name="IRS990ScheduleCType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule C</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Political Expenditures -->			
			<xsd:element name="PoliticalExpendituresAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Political expenditures</Description>
						<LineNumber>Part I-A Line 2</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Volunteer Hours -->			
			<xsd:element name="VolunteerHoursCnt" type="IntegerNNType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Volunteer hours</Description>
						<LineNumber>Part I-A Line 3</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Amount of 4955 Tax -->			
			<xsd:element name="Section4955OrganizationTaxAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Enter the amount of any excise tax incurred under section 4955</Description>
						<LineNumber>Part I-B Line 1</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Amount of 4955 Tax On Managers -->			
			<xsd:element name="Section4955ManagersTaxAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Enter the amount of any excise tax incurred by organization managers under section 4955</Description>
						<LineNumber>Part I-B Line 2</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Form 4720 Filed? -->			
			<xsd:element name="Form4720FiledSection4955TaxInd" type="BooleanType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description/>
						<LineNumber>Part I-B Line 3</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Correction Made? -->			
			<xsd:element name="CorrectionMadeInd" type="BooleanType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>If the filing organization incurred in a section 4955 tax, did it file Form 4720 for this year?</Description>
						<LineNumber>Part I-B Line 4a</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Amount Expended For 527 Activities -->			
			<xsd:element name="Expended527ActivitiesAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Enter the amount directly expended by the filing organization for section 527 exempt function activities</Description>
						<LineNumber>Part I-C Line 1</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Amount of Internal Funds Contributed -->			
			<xsd:element name="InternalFundsContributedAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Enter the amount of the filing organization's own internal funds contributed to other organizations for section 527 exempt function activities</Description>
						<LineNumber>Part I-C Line 2</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Total Exempt Function Expenditures -->			
			<xsd:element name="TotalExemptFunctionExpendAmt" type="USAmountType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Total of direct and indirect exempt function expenditures. Add lines 1 and 2</Description>
						<LineNumber>Part I-C Line 3</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Form 1120POL Filed? -->			
			<xsd:element name="Form1120POLFiledInd" type="BooleanType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>Did the filing organization file Form 1120-POL for this year?</Description>
						<LineNumber>Part I-C Line 4</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>

			<!-- Section 527 Political Organizations Table -->
			<xsd:element name="Section527PoliticalOrgGrp" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>State the names, addresses and Employer Identification Number (EIN) of all section 527 political organizations to which payments were made</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<!-- Name of 527 Organization -->			
						<xsd:element name="OrganizationBusinessName" type="BusinessNameType" minOccurs="0">			
							<xsd:annotation>		
								<xsd:documentation>	
									<Description>Name of 527 organization</Description>
									<LineNumber>Part I-C Line 5(a)</LineNumber>
								</xsd:documentation>	
							</xsd:annotation>		
						</xsd:element>

						<!-- Address of 527 Organization -->
						<!-- Address Choice -->
						<xsd:choice>
							<xsd:element name="USAddress" type="USAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address - US</Description>
										<LineNumber>Part I-C Line 5(b)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ForeignAddress" type="ForeignAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address - Foreign</Description>
										<LineNumber>Part I-C Line 5(b)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>

						<!-- EIN -->			
						<xsd:element name="EIN" type="EINType" minOccurs="0">			
							<xsd:annotation>		
								<xsd:documentation>	
									<Description>EIN</Description>
									<LineNumber>Part I-C Line 5(c)</LineNumber>
								</xsd:documentation>	
							</xsd:annotation>		
						</xsd:element>

						<!-- Amount Paid From Internal Funds -->			
						<xsd:element name="PaidInternalFundsAmt" type="USAmountType" minOccurs="0">			
							<xsd:annotation>		
								<xsd:documentation>	
									<Description>Amount paid from internal funds</Description>
									<LineNumber>Part I-C Line 5(d)</LineNumber>
								</xsd:documentation>	
							</xsd:annotation>		
						</xsd:element>

						<!-- Amount of Contributions Received and Delivered -->			
						<xsd:element name="ContributionsRcvdDlvrAmt" type="USAmountType" minOccurs="0">			
							<xsd:annotation>		
								<xsd:documentation>	
									<Description>Amount of contributions received and delivered</Description>
									<LineNumber>Part I-C Line 5(e)</LineNumber>
								</xsd:documentation>	
							</xsd:annotation>		
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Part II-A : Political Campaign and Lobbying Activities -->
			<!-- to be completed by organizations exempt under section 501(c)(3) that filed Form 5768 -->
					
			<!-- Check If the Organization Belongs to an Affiliated Group -->
			<xsd:element name="OrganizationBelongsAffltGrpInd" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check if the organization belongs to an affiliated group</Description>
						<LineNumber>Part II-A Line A</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="CheckboxType">
							<xsd:attribute name="referenceDocumentId" type="IdListType"/>
							<xsd:attribute name="referenceDocumentName" type="StringType" fixed="AffiliatedGroupAttachment AffiliatedGroupSchedule AveragingAttachment"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>

			<!-- Check If "A" is Checked and "Limited Control" Provisions Apply -->
			<xsd:element name="LimitedControlProvisionsAppInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Check if "A" is checked and "limited control" provisions apply</Description>
						<LineNumber>Part II-A Line B</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Lobbying Expenditures to Influence Public Opinion (Grassroots Lobbying) -->
			<xsd:element name="TotalGrassrootsLobbyingGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total lobbying expenditures to influence public opinion (grassroots lobbying)</Description>
						<LineNumber>Part II-A Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Lobbying Expenditures to Influence a Legislative Body (Direct Lobbying) -->
			<xsd:element name="TotalDirectLobbyingGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total lobbying expenditures to influence a legislative body (direct lobbying)</Description>
						<LineNumber>Part II-A Line 1b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Lobbying Expenditures (Add Lines 1a and 1b) -->
			<xsd:element name="TotalLobbyingExpendGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total lobbying expenditures (add lines 1a and 1b)</Description>
						<LineNumber>Part II-A Line 1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Exempt Purpose Expenditures -->
			<xsd:element name="OtherExemptPurposeExpendGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other exempt purpose expenditures</Description>
						<LineNumber>Part II-A Line 1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total exempt purpose expenditures (add lines 1c and 1d) -->
			<xsd:element name="TotalExemptPurposeExpendGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total exempt purpose expenditures (add lines 1c and 1d)</Description>
						<LineNumber>Part II-A Line 1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lobbying Nontaxable Amount -->
			<xsd:element name="LobbyingNontaxableAmountGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Lobbying nontaxable amount</Description>
						<LineNumber>Part II-A Line 1f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grassroots Nontaxable Amount (Enter 25% of Line 1f) -->
			<xsd:element name="GrassrootsNontaxableGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grassroots nontaxable amount (enter 25% of line 1f)</Description>
						<LineNumber>Part II-A Line 1g</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Subtract Line 1g From Line 1a. Enter -0- If Line 1g is More Than Line 1a -->
			<xsd:element name="TotLbbyngGrassrootMnsNonTxGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 1g from line 1a. Enter -0- if line 1g is more than line 1a</Description>
						<LineNumber>Part II-A Line 1h</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Subtract Line 1f From Line 1c. Enter -0- If Line 1f is More Than Line 1c -->
			<xsd:element name="TotLbbyExpendMnsLbbyngNonTxGrp" type="Form990SchCPartIIAGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Subtract line 1f from line 1c. Enter -0- if line 1f is more than line 1c</Description>
						<LineNumber>Part II-A Line 1i</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount Other Than Zero on Either Line 1h or Line 1i, Did the Organization File Form 4720? -->
			<xsd:element name="Form4720FiledInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If there is an amount other than zero on either line h or line i, did the organization file Form 4720 reporting section 4911 tax for this year?</Description>
						<LineNumber>Part II-A Line 1j</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lobbying Nontaxable Amount -->
			<xsd:element name="AvgLobbyingNontaxableAmountGrp" type="Form990SchCPartIIAGroup2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Lobbying nontaxable amount</Description>
						<LineNumber>Part II-A Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lobbying ceiling amount (150% of line 2a(e)) -->
			<xsd:element name="LobbyingCeilingAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Lobbying ceiling amount (150% of line 2a(e))</Description>
						<LineNumber>Part II-A Line 2b(e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Lobbying Expenditures -->
			<xsd:element name="AvgTotalLobbyingExpendGrp" type="Form990SchCPartIIAGroup2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total lobbying expenditures</Description>
						<LineNumber>Part II-A Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grassroots Nontaxable Amount -->
			<xsd:element name="AvgGrassrootsNontaxableGrp" type="Form990SchCPartIIAGroup2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grassroots nontaxable amount</Description>
						<LineNumber>Part II-A Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grassroots Ceiling Amount (150% of Line 2d(e)) -->
			<xsd:element name="GrassrootsCeilingAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grassroots ceiling amount (150% of line 2d(e))</Description>
						<LineNumber>Part II-A Line 2e(e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grassroots Lobbying Expenditures -->
			<xsd:element name="AvgGrassrootsLobbyingExpendGrp" type="Form990SchCPartIIAGroup2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grassroots lobbying expenditures</Description>
						<LineNumber>Part II-A Line 2f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part II-B : Political Campaign and Lobbying Activities -->
			<!-- to be completed by organizations exempt under section 501(c) except those organizations exempt under section 501(c)(3) that filed Form 5768 (election under section 501(h)) -->

			<!-- During the year, did the filing organization attempt to influence foreign, national, state or local legislation, including any attempt to influence public opinion on a legislative matter or referendum, through the use of: -->

			<!-- Volunteers? -->
			<xsd:element name="VolunteersInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Volunteers?</Description>
						<LineNumber>Part II-B Line 1a Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Paid Staff or Management? -->
			<xsd:element name="PaidStaffOrManagementInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Paid staff or management?</Description>
						<LineNumber>Part II-B Line 1b Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Media Advertisements? -->
			<xsd:element name="MediaAdvertisementsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Media advertisements?</Description>
						<LineNumber>Part II-B Line 1c Column(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Media Advertisements Amount -->
			<xsd:element name="MediaAdvertisementsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Media advertisements amount (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1c Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Mailings to Members, Legislators, or to the Public? -->
			<xsd:element name="MailingsMembersInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Mailings to members, legislators, or to the public?</Description>
						<LineNumber>Part II-B Line 1d Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Mailings to Members, Legislators, or to the Public Amount -->
			<xsd:element name="MailingsMembersAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Media advertisements amount (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1d Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Publications, or Published or Broadcast Statements? -->
			<xsd:element name="PublicationsOrBroadcastInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Publications, or published or broadcast statements?</Description>
						<LineNumber>Part II-B Line 1e Column(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Mailings to Members, Legislators, or to the Public Amount -->
			<xsd:element name="PublicationsOrBroadcastAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Publications, or published or broadcast statements (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1e Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants to Other Organizations for Lobbying Purposes? -->
			<xsd:element name="GrantsOtherOrganizationsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants to other organizations for lobbying purposes?</Description>
						<LineNumber>Part II-B Line 1f Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Grants to Other Organizations for Lobbying Purposes Amount -->
			<xsd:element name="GrantsOtherOrganizationsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants to other organizations for lobbying purposes (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1f Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Direct Contact With Legislators? -->
			<xsd:element name="DirectContactLegislatorsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Direct contact with legislators, their staffs, government officials, or a legislative body?</Description>
						<LineNumber>Part II-B Line 1g Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Direct Contact With Legislators Amount -->
			<xsd:element name="DirectContactLegislatorsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Direct contact with legislators, their staffs, government officials, or a legislative body (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1g Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Rallies, Demonstrations? -->
			<xsd:element name="RalliesDemonstrationsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Rallies, demonstrations, seminars, conventions, speeches, lectures, or any other means?</Description>
						<LineNumber>Part II-B Line 1h Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Rallies, Demonstrations Amount -->
			<xsd:element name="RalliesDemonstrationsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Rallies, demonstrations, seminars, conventions, speeches, lectures, or any other means (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1h Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Activities? -->
			<xsd:element name="OtherActivitiesInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other activities?</Description>
						<LineNumber>Part II-B Line 1i Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Other Activities Amount -->
			<xsd:element name="OtherActivitiesAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other activities amount</Description>
						<LineNumber>Part II-B Line 1i Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Lobbying Expenditures -->
			<xsd:element name="TotalLobbyingExpendituresAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Line 1c through 1i (only for section 501(c)(3))</Description>
						<LineNumber>Part II-B Line 1j Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Not Described in 501(c)(3)? -->
			<xsd:element name="NotDescribedSection501c3Ind" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the activities in line 1 cause the organization to be not described in section 501(c)(3)?</Description>
						<LineNumber>Part II-B Line 2a Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount of 4912 Tax -->
			<xsd:element name="Tax4912Amt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If "Yes," enter the amount of any tax incurred under section 4912</Description>
						<LineNumber>Part II-B Line 2b Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount of Manager's 4912 Tax -->
			<xsd:element name="Managers4912TaxAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If "Yes," enter the amount of any tax incurred by organization managers under section 4912</Description>
						<LineNumber>Part II-B Line 2c Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Form 4720 Filed? -->			
			<xsd:element name="Form4720Filed4912TaxInd" type="BooleanType" minOccurs="0">			
				<xsd:annotation>		
					<xsd:documentation>	
						<Description>If the filing organization incurred a section 4912 tax, did it file Form 4720 for this year?</Description>
						<LineNumber>Part II-B Line 2d Column (a)</LineNumber>
					</xsd:documentation>	
				</xsd:annotation>		
			</xsd:element>
			
			<!-- Part III-A : Political Campaign and Lobbying Activities -->
			<!-- to be completed by all organizations exempt under section 501(c)(4), section 501(c)(5), or section 501(c)(6) -->

			<!-- Substantially All Dues Nondeductible? -->
			<xsd:element name="SubstantiallyAllDuesNondedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Were substantially all (90% or more) dues received nondeductible by members?</Description>
						<LineNumber>Part III-A Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Only In House Lobbying? -->
			<xsd:element name="OnlyInHouseLobbyingInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization make only in-house lobbying expenditures of $2,000 or less?</Description>
						<LineNumber>Part III-A Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Agree to Carryover Prior Year? -->
			<xsd:element name="AgreeCarryoverPriorYearInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization agree to carryover lobbying and political expenditures from the prior year?</Description>
						<LineNumber>Part III-A Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part III-B : Political Campaign and Lobbying Activities -->
			<!-- to be completed by all organizations exempt under section 501(c)(4), section 501(c)(5), or section 501(c)(6) if BOTH questions 1 and 2 are answered "No" OR if question 3 is answered "Yes" -->

			<!-- Dues, Assessments, Etc. -->
			<xsd:element name="DuesAssessmentsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Dues, assessments and similar amounts from members</Description>
						<LineNumber>Part III-B Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Lines 2a - 2c: Section 162(e) non-deductible lobbying and political expenditures (do not include amounts of politiical expenses for which the section 527(f) tax was paid) -->
			<!-- Non-Deductible Lobbying & Political Exp.- Current -->
			<xsd:element name="NonDeductibleLbbyngPltclCYAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year</Description>
						<LineNumber>Part III-B Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Deductible Lobbying & Political Exp.- Carryover -->
			<xsd:element name="NonDedLbbyngPltclCyovAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Carryover from last year</Description>
						<LineNumber>Part III-B Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Non-Deductible Lobbying & Political Exp.- Total -->
			<xsd:element name="NonDeductibleLbbyngPltclTotAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total</Description>
						<LineNumber>Part III-B Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Aggregate Amount Reported In Dues Notices -->
			<xsd:element name="AggregateReportedDuesNtcAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Aggregate amount reported in section 6033(e)(1)(A) notices of nondeductible section 162(e) dues</Description>
						<LineNumber>Part III-B Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Amount to Be Carried Over -->
			<xsd:element name="CarriedOverAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If notices were sent and the amount on line 2c exceeds the amount on line 3, what portion of that amount does the organization agree to carryover to the reasonable estimate of nondeductible lobbying and political expenditure next year?</Description>
						<LineNumber>Part III-B Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Taxable Amount -->
			<xsd:element name="TaxableAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Taxable amount of lobbying and political expenditures (line 2c total minus 3 and 4)</Description>
						<LineNumber>Part III-B Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part IV : Explanations -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="5">		
				<xsd:annotation>
					<xsd:documentation>Part IV contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>		
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
								<LineNumber>Part IV</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference explanation</Description>
								<LineNumber>Part IV</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>	
				</xsd:complexType>		
			</xsd:element>		

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- =========================================================================== -->
	<!-- ============= Local types, types for repeating groups etc. ================ -->
	<!-- =========================================================================== -->
	
	<!-- Type for Part II-A, Line 1a through 1i, Columns (a) and (b) -->
	<xsd:complexType name="Form990SchCPartIIAGroup1Type">
		<xsd:sequence>
			<!-- Filing Organization's Totals -->
			<xsd:element name="FilingOrganizationsTotalAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Filing organization's totals</Description>
						<LineNumber>Part II-A Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Affiliated Group Totals -->
			<xsd:element name="AffiliatedGroupTotalAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Affiliated group totals</Description>
						<LineNumber>Part II-A Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- Type for Part II-A, Lines 2a, 2c, 2d and 2f -->
	<xsd:complexType name="Form990SchCPartIIAGroup2Type">
		<xsd:sequence>
			<!-- Current Year Minus 3 -->
			<xsd:element name="CurrentYearMinus3Amt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus 3</Description>
						<LineNumber>Part II-A Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Year Minus 2 -->
			<xsd:element name="CurrentYearMinus2Amt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus 2</Description>
						<LineNumber>Part II-A Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Year Minus 1 -->
			<xsd:element name="CurrentYearMinus1Amt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus 1</Description>
						<LineNumber>Part II-A Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Current Year -->
			<xsd:element name="CurrentYearAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year</Description>
						<LineNumber>Part II-A Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Total -->
			<xsd:element name="TotalAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total</Description>
						<LineNumber>Part II-A Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule D ================ -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleD">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule D</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleDType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleD">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule D (Form 990) Supplemental Financial Statements -->
	<xsd:complexType name="IRS990ScheduleDType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule D</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- Total Number of Donor Advised Funds Maintained -->
			<xsd:element name="DonorAdvisedFundsHeldCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the total number of donor advised funds maintained at the end of the tax year</Description>
						<LineNumber>Part I Line 1 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Number of Separate Accounts for Participating Donors -->
			<xsd:element name="FundsAndOtherAccountsHeldCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the total number of separate accounts for participating donors where donors have the right to provide advice on the use or distribution of funds owned at the end of the tax year</Description>
						<LineNumber>Part I Line 1 Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Contributions, Gifts, Grants, and Similar Amounts Received: Donor Advised Funds -->
			<xsd:element name="DonorAdvisedFundsContriAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Contributions, gifts, grants, and similar amounts received: donor advised funds</Description>
						<LineNumber>Part I Line 2 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Contributions to Funds & Other Accounts -->
			<xsd:element name="FundsAndOtherAccountsContriAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Contributions, gifts, grants, and similar amounts received: funds and other accounts</Description>
						<LineNumber>Part I Line 2 Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Grants and Allocations From Donor Advised Funds -->
			<xsd:element name="DonorAdvisedFundsGrantsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants and allocations from donor advised funds</Description>
						<LineNumber>Part I Line 3 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<!-- Grants from Funds and Other Accounts -->
			<xsd:element name="FundsAndOtherAccountsGrantsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Grants from funds and other accounts</Description>
						<LineNumber>Part I Line 3 Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<!-- Aggregate Value of Assests Held in all Donor Advised Funds -->
			<xsd:element name="DonorAdvisedFundsVlEOYAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the aggregate value of assets held in all donor advised funds at the end of the tax year</Description>
						<LineNumber>Part I Line 4 Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Aggregate Value of Assests Held in all Separate Accounts for Participating Donors  -->
			<xsd:element name="FundsAndOtherAccountsVlEOYAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter the aggregate value of assets held in all funds and other accounts at the end of the tax year</Description>
						<LineNumber>Part I Line 4 Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Disclosed Organization's Legal Control? -->
			<xsd:element name="DisclosedOrgLegCtrlInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization inform all donors and donor advisors in writing that the assets held in DAFs are the organization's property, subject to the organization's exclusive legal control?</Description>
						<LineNumber>Part I Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Disclosed for Charitable Purposes Only? -->
			<xsd:element name="DisclosedForCharitablePrpsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization inform all grantees, donors, and donor advisors in writing that grant funds may be used only for charitable purposes and not for the benefit of the donor or donor advisor?</Description>
						<LineNumber>Part I Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part II Conservation Easements -->
			<!-- Preservation for Public Use -->
			<xsd:element name="PreservationPublicUseInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Preservation for  public use</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Protection of Natural Habitat -->
			<xsd:element name="ProtectionNaturalHabitatInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Protection of natural habitat</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Preservation of Open Space -->
			<xsd:element name="PreservationOpenSpaceInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Preservation of open space</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Historic Land Area -->
			<xsd:element name="HistoricLandAreaInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Historic land area</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Historic Structure -->
			<xsd:element name="HistoricStructureInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Historic structure</Description>
						<LineNumber>Part II Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Number of Easements -->
			<xsd:element name="TotalEasementsCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total number of easements</Description>
						<LineNumber>Part II Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Acreage -->
			<xsd:element name="TotalAcreageCnt" type="DecimalNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total acreage subject to easements</Description>
						<LineNumber>Part II Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Number of Historic Structure Easements -->
			<xsd:element name="HistoricStructureEasementsCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of easements on a certified historic structure included in (a)</Description>
						<LineNumber>Part II Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Number Historic Structure Easements Acquired -->
			<xsd:element name="HistoricStrctrEasementsAftrCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of historic structure easements acquired after 8-17-2006</Description>
						<LineNumber>Part II Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Number Easements Modified, Etc. -->
			<xsd:element name="ModifiedEasementsCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of conservation easements modified, transferred, released, or terminated by the organization during the taxable year</Description>
						<LineNumber>Part II Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Number States in Which Easements Held -->
			<xsd:element name="StatesEasementsHeldCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of states in which the organization held an easement</Description>
						<LineNumber>Part II Line 4</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Written Policy in Reference to Monitoring, Etc? -->
			<xsd:element name="WrittenPolicyMonitoringInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does the organization have a written policy regarding the periodic monitoring, inspection, and enforcement of the easements it holds?</Description>
						<LineNumber>Part II Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Staff Hours Spent on Enforcement -->
			<xsd:element name="StaffHoursSpentEnforcementCnt" type="DecimalNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Staff hours devoted to monitoring or enforcing easements during the year</Description>
						<LineNumber>Part II Line 6</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Expenses Incurred for Enforcement -->
			<xsd:element name="ExpensesIncurredEnforcementAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description> Amount of expenses incurred in monitoring or enforcing easements during the taxable year</Description>
						<LineNumber>Part II Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Section 170(h) Requirements Satisfied? -->
			<xsd:element name="Section170hRqrStsfdInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does each conservation easement reported on line 2(d) above satisfy the requirements of section 170(h)(4)(B)(i) and 170(h)(4)(B)(ii)?</Description>
						<LineNumber>Part II Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Organizations Maintaining Collections of Art, Historical Treasures, or Other Similar Assets -->
			<!-- Works of Art, Historical Treasures, or Other Similar Assets Held for Public Exhibition, Education or Research in Furtherance of Public Service -->
			<xsd:element name="ArtPublicExhibitionAmountsGrp" type="Form990SchDPartIIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If the organization elected, as permitted under SFAS 116, to report in its revenue statement and balance sheet works of art, historical treasures, or other similar assets held for public exhibition, education or research in furtherance of public service, provide the following amounts relating to these items</Description>
						<LineNumber>Part III Line 1b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Received or Held Works of Art, Historical Treasures, or Other Similar Assets for Financial Gain -->
			<xsd:element name="HeldWorksArtGrp" type="Form990SchDPartIIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If the organization received or held works of art, historical treasures, or other similar assets for financial gain, provide the following amounts required to be reported under SFAS 116 relating to these items</Description>
						<LineNumber>Part III Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Collection Used For Public Exhibition -->
			<xsd:element name="CollectionUsedPubExhibitionInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collection used for public exhibition</Description>
						<LineNumber>Part III Line 3a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Collection Used For Scholarly Research -->
			<xsd:element name="CollUsedScholarlyRsrchInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collection used for scholarly research</Description>
						<LineNumber>Part III Line 3b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Collection Used For Preservation -->
			<xsd:element name="CollectionUsedPreservationInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collection used for preservation</Description>
						<LineNumber>Part III Line 3c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Collection Used For Loan or Exchange Programs -->
			<xsd:element name="CollUsedLoanOrExchProgInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collection used for loan or exchange programs</Description>
						<LineNumber>Part III Line 3d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Collection Used for Other Purposes and Description -->
			<xsd:element name="CollectionUsedOtherPurposesGrp" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Collection used for other purposes and description</Description>
						<LineNumber>Part III Line 3e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType> 
					<xsd:sequence>
						<xsd:element name="CollectionUsedOtherPurposesInd" type="CheckboxType"/>
						<xsd:element name="OtherPurposesDesc" type="LineExplanationType"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<!-- Line 4, No Schema Implementation, Instruction Only -->
			<!-- Solicited Assets For Sale? -->
			<xsd:element name="SolicitedAssetsSaleInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>During the year, did the organization solicit or receive donations of art, historical treasures or other similar assets to be sold to raise funds rather than to be maintained as part of the organization's collection?</Description>
						<LineNumber>Part III Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part IV Trust, Escrow and Custodial Arrangements -->
			<!-- Agent, Trustee, Etc? -->
			<xsd:element name="AgentTrusteeEtcInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is the organization an agent, trustee, custodian or other intermediary for contributions or other assets not included on Form 990, Part X?</Description>
						<LineNumber>Part IV Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Beginning Balance -->
			<xsd:element name="BeginningBalanceAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Beginning balance</Description>
						<LineNumber>Part IV Line 1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Additions During Year -->
			<xsd:element name="AdditionsDuringYearAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Additions during the year</Description>
						<LineNumber>Part IV Line 1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Distributions During Year -->
			<xsd:element name="DistributionsDuringYearAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Distributions during the year</Description>
						<LineNumber>Part IV Line 1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Ending Balance -->
			<xsd:element name="EndingBalanceAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Ending balance</Description>
						<LineNumber>Part IV Line 1f</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Did the Organization Include sn Amount on Form 990, Part X, Line 21? -->
			<xsd:element name="InclEscrowCustodialAcctLiabInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization include an amount on Form 990, Part X, line 21?</Description>
						<LineNumber>Part IV Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part XIII Contains an Explanation for Part IV, Line 2b -->
			<xsd:element name="ExplanationProvidedInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part XIII contains an explanation for Part IV, Line 2b</Description>
						<LineNumber>Part IV Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part V Endowment Funds -->
			<!-- Current Year -->
			<xsd:element name="CYEndwmtFundGrp" type="Form990SchDPartVGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year</Description>
						<LineNumber>Part V Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Prior Year -->			
			<xsd:element name="CYMinus1YrEndwmtFundGrp" type="Form990SchDPartVGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus one year</Description>
						<LineNumber>Part V Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Two Years Back -->			
			<xsd:element name="CYMinus2YrEndwmtFundGrp" type="Form990SchDPartVGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus two years</Description>
						<LineNumber>Part V Column (c)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Three Years Back -->			
			<xsd:element name="CYMinus3YrEndwmtFundGrp" type="Form990SchDPartVGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus three years</Description>
						<LineNumber>Part V Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Four Years Back -->			
			<xsd:element name="CYMinus4YrEndwmtFundGrp" type="Form990SchDPartVGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Current year minus four years</Description>
						<LineNumber>Part V Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Provide the Estimated Percentage of the Year End Balances Held as: -->
			<!-- Board Designated EOY Balance -->
			<xsd:element name="BoardDesignatedBalanceEOYPct" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Board designated EOY balance</Description>
						<LineNumber>Part V Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Permanent Endowment EOY Balance -->
			<xsd:element name="PrmnntEndowmentBalanceEOYPct" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Permanent endowment EOY balance</Description>
						<LineNumber>Part V Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Term Endowment EOY Balance -->
			<xsd:element name="TermEndowmentBalanceEOYPct" type="RatioType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Term endowment EOY balance</Description>
						<LineNumber>Part V Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Endowments Held by Unrelated Organizations? -->
			<xsd:element name="EndowmentsHeldUnrelatedOrgInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Endowments held by unrelated organizations?</Description>
						<LineNumber>Part V Line 3a(i)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Endowments Held by Related Organizations? -->
			<xsd:element name="EndowmentsHeldRelatedOrgInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Endowments held by related organizations?</Description>
						<LineNumber>Part V Line 3a(ii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Are Related Organizations Listed On Schedule R? -->
			<xsd:element name="RelatedOrgListSchRInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Are related orgs listed on Schedule R?</Description>
						<LineNumber>Part V Line 3b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part VI Land, Buildings and Equipment -->
			<!-- Land  -->
			<xsd:element name="LandGrp" type="Form990SchDPartVIGroup2Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Land</Description>
						<LineNumber>Part VI Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Buildings -->
			<xsd:element name="BuildingsGrp" type="Form990SchDPartVIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Buildings</Description>
						<LineNumber>Part VI Line 1b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Leasehold Improvements -->
			<xsd:element name="LeaseholdImprovementsGrp" type="Form990SchDPartVIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Leasehold improvements</Description>
						<LineNumber>Part VI Line 1c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Equipment -->
			<xsd:element name="EquipmentGrp" type="Form990SchDPartVIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Equipment</Description>
						<LineNumber>Part VI Line 1d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other -->
			<xsd:element name="OtherLandBuildingsGrp" type="Form990SchDPartVIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other</Description>
						<LineNumber>Part VI Line 1e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total of Book Value -->
			<xsd:element name="TotalBookValueLandBuildingsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of book value</Description>
						<LineNumber>Part VI Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part VII -->
			<!-- Financial Derivatives -->
			<xsd:element name="FinancialDerivativesGrp" type="Form990SchDPartVIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Financial derivatives</Description>
						<LineNumber>Part VII Column(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Closely Held Equity Interests -->
			<xsd:element name="CloselyHeldEquityInterestsGrp" type="Form990SchDPartVIIGroup1Type" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Closely held equity interests</Description>
						<LineNumber>Part VII Column(a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other -->
			<xsd:element name="OtherSecuritiesGrp" type="Form990SchDPartVIIGroup2Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other</Description>
						<LineNumber>Part VII Column (a)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total of Book Value -->
			<xsd:element name="TotalBookValueSecuritiesAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of book value</Description>
						<LineNumber>Part VII Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part VIII Investments - Program Related -->
			<!-- Description -->
			<xsd:element name="InvstProgramRelatedOrgGrp" type="Form990SchDPartVIIIGroup1Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investments program related table</Description>
						<LineNumber>Part VIII</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total of Book Value -->
			<xsd:element name="TotalBookValueProgramRltdAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of book value</Description>
						<LineNumber>Part VIII Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part IX Other Assets -->
			<xsd:element name="OtherAssetsOrgGrp" type="Form990SchDPartIXGroup1Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other assets</Description>
						<LineNumber>Part IX</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Book Value -->
			<xsd:element name="TotalBookValueOtherAssetsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total book value</Description>
						<LineNumber>Part IX Column (b)</LineNumber>						
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part X Other Liabilities -->
			<!-- Federal Income Tax Liability -->
			<xsd:element name="FederalIncomeTaxLiabilityAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Federal income tax liability</Description>
						<LineNumber>Part X Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Liabilities -->
			<xsd:element name="OtherLiabilitiesOrgGrp" type="Form990SchDPartXGroup1Type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other liabilities</Description>
						<LineNumber>Part X</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total of Amounts -->
			<xsd:element name="TotalLiabilityAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total of liability amounts</Description>
						<LineNumber>Part X Column (b)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part XIII contains the text of the footnote for Part X, Line 2 -->
			<xsd:element name="FootnoteTextInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part XIII contains the text of the footnote for Part X, Line 2</Description>
						<LineNumber>Part X Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part XI : Reconciliation of Revenue per Audited Financial Statements with Revenue per Return -->
			<!-- Total Revenue, Gains & Other Support per Audited Financial Statments -->
			<xsd:element name="TotalRevEtcAuditedFinclStmtAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total revenue, gains and other support per audited financial statments</Description>
						<LineNumber>Part XI Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Net Unrealized Gains on Investments -->
			<xsd:element name="NetUnrealizedGainsInvstAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Net unrealized gains on investments</Description>
						<LineNumber>Part XI Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Donated Services and Use of Facilities -->
			<xsd:element name="DonatedServicesAndUseFcltsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Donated services and use of facilities</Description>
						<LineNumber>Part XI Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Recoveries of Prior Year Grants -->
			<xsd:element name="RecoveriesPriorYearGrantsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Recoveries of prior year grants</Description>
						<LineNumber>Part XI Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Revenues -->
			<xsd:element name="OtherRevenueAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other revenues</Description>
						<LineNumber>Part XI Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Amounts Line 2a through 2d  -->
			<xsd:element name="RevenueNotReportedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total amounts, add lines 2a through 2d</Description>
						<LineNumber>Part XI Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Subtract Line 2e from Line 1 -->
			<xsd:element name="RevenueSubtotalAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part XI line 1 minus Part XI line 2e</Description>
						<LineNumber>Part XI Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Amounts Included on Form 990, Part VIII, Line 12, but not on Line 1: -->
			<!-- Investment Expenses not Included -->
			<xsd:element name="InvestmentExpensesNotIncldAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment expenses not included on Form 990, Part VIII, line 7b</Description>
						<LineNumber>Part XI Line 4a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Non-included Revenues -->
			<xsd:element name="OtherRevenuesNotIncludedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other revenues not included</Description>
						<LineNumber>Part XI Line 4b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Amounts Line 4a and 4b  -->
			<xsd:element name="RevenueNotReportedFinclStmtAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total amounts, add lines 4a and 4b</Description>
						<LineNumber>Part XI Line 4c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Revenue Add Lines 3 and 4c -->
			<xsd:element name="TotalRevenuePerForm990Amt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total revenue (Form 990, Part 1, Line 12). Add lines 3 and 4c</Description>
						<LineNumber>Part XI Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part XII : Reconciliation of Expenses per Audited Financial Statements with Expenses per Return -->
			<!-- Total Expenses, and Losses per Audited Financial Statments -->
			<xsd:element name="TotExpnsEtcAuditedFinclStmtAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total Expenses, and Losses per audited financial statments</Description>
						<LineNumber>Part XII Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Amounts included on line 1 but not on Form 990, Part VIII, line 12 -->
			<!-- Donated Services and Use of Facilities -->
			<xsd:element name="DonatedServicesUseFcltsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Donated Services and Use of Facilities</Description>
						<LineNumber>Part XII Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Prior Year Adjustments Reported on Form 990 -->
			<xsd:element name="PriorYearAdjustmentsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Prior year adjustments</Description>
						<LineNumber>Part XII Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Losses Reported on Form 990 -->
			<xsd:element name="LossesReportedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Losses reported</Description>
						<LineNumber>Part XII Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Expenses Included -->
			<xsd:element name="OtherExpensesIncludedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other expenses included</Description>
						<LineNumber>Part XII Line 2d</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Amounts Line 2a through 2d  -->
			<xsd:element name="ExpensesNotReportedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total amounts, add lines 2a through 2d</Description>
						<LineNumber>Part XII Line 2e</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Subtract Line 2e from Line 1 -->
			<xsd:element name="ExpensesSubtotalAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part XII line 1 minus Part XII line 2e</Description>
						<LineNumber>Part XII Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Amounts Included on Form 990, Part IX, Line 25, but not on Line 1: -->
			<!-- Investment Expenses not Included -->
			<xsd:element name="InvestmentExpensesNotIncld2Amt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Investment expenses not included on Form 990, Part VIII, line 7b</Description>
						<LineNumber>Part XII Line 4a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Other Expenses -->
			<xsd:element name="OtherExpensesNotIncludedAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Other expenses</Description>
						<LineNumber>Part XII Line 4b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Amounts Line 4a through 4b  -->
			<xsd:element name="ExpensesNotRptFinclStmtAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total amounts, add lines 4a through 4b</Description>
						<LineNumber>Part XII Line 4c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Total Expenses - Add Lines 3 and 4c -->
			<xsd:element name="TotalExpensesPerForm990Amt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total expenses (Form 990, Part I, Line 18). Add lines 3 and 4c</Description>
						<LineNumber>Part XII Line 5</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Part XIII Supplemental Information -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="15">		
				<xsd:annotation>
