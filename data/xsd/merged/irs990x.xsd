				<xsd:annotation>
					<xsd:documentation>
						<Description>Net assets or fund balance at end of year</Description>
						<LineNumber>Part XI, Line 10</LineNumber>
					</xsd:documentation>
				</xsd:annotation>	
			</xsd:element -->

			<!-- Part XII Financials --> 

			<!-- Schedule O Indicator -->
			<xsd:element name="InfoInScheduleOPartXIIInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Schedule O contains a response to a question in Part XII</Description>
						<LineNumber>Part XII</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Method of Accounting -->
			<xsd:choice minOccurs="1">
				<!-- Cash -->
				<xsd:element name="MethodOfAccountingCashInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Cash</Description>
							<LineNumber>Part XII Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Method of Accounting - Accrual -->
				<xsd:element name="MethodOfAccountingAccrualInd" type="CheckboxType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Accrual</Description>
							<LineNumber>Part XII Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<!-- Method of Accounting - Other -->
				<xsd:element name="MethodOfAccountingOtherInd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Method of accounting - Other</Description>
							<LineNumber>Part XII Line 1</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:simpleContent>
							<xsd:extension base="CheckboxType">
								<xsd:attribute name="methodOfAccountingOtherDesc" type="ShortDescriptionType"/>
							</xsd:extension>
						</xsd:simpleContent>
					</xsd:complexType>
				</xsd:element>
			</xsd:choice>

			<!-- Accountant Provide Compilation or Review? -->
			<xsd:element name="AccountantCompileOrReviewInd" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Accountant provide compilation or review?</Description>
						<LineNumber>Part XII Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Accountant Provide Compilation or Review Basis -->
			<xsd:element name="AcctCompileOrReviewBasisGrp" type="FinancialStatementType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Basis in which the financial statements were compiled or reviewed by independent accountant</Description>
						<LineNumber>Part XII Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Financial Sheets Audited? -->
			<xsd:element name="FSAuditedInd" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Financial sheets audited?</Description>
						<LineNumber>Part XII Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Financial Sheets Audited Basis -->
			<xsd:element name="FSAuditedBasisGrp" type="FinancialStatementType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Basis in which the financial statements were audited by an independent accountant</Description>
						<LineNumber>Part XII Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Audit Committee? -->
			<xsd:element name="AuditCommitteeInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does the organization have an audit committee?</Description>
						<LineNumber>Part XII Line 2c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Federal Grant Audit Required? -->
			<xsd:element name="FederalGrantAuditRequiredInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Federal grant audit required?</Description>
						<LineNumber>Part XII Line 3a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Federal Grant Audit Performed? -->
			<xsd:element name="FederalGrantAuditPerformedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Federal grant audit performed?</Description>
						<LineNumber>Part XII Line 3b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

