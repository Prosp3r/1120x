					</xsd:annotation>
				</xsd:element>
				
				<xsd:element name="ContributorPersonNm" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Contributor name - Individual</Description>
							<LineNumber>Part I Column (b)</LineNumber>
