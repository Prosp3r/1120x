					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Manner of Cash Disbursement -->
			<xsd:element name="MannerOfCashDisbursementTxt" type="LineExplanationType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Manner of cash disbursement</Description>
						<LineNumber>Column (e)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
