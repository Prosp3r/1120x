	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule G ========================== -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleG">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule G</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleGType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleG">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule G (Form 990) Supplemental Information Regarding Fundraising or Gaming Activities -->

	<xsd:complexType name="IRS990ScheduleGType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule G</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- Part I Fundraising Activities -->
			<!-- Indicate whether the organization raised funds through any of the following activities. (Check all that apply) -->
			<!-- Mail Solicitations -->
			<xsd:element name="MailSolicitationsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Mail solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Email Solicitations -->
			<xsd:element name="EmailSolicitationsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Email solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Phone Solicitations -->
			<xsd:element name="PhoneSolicitationsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Phone solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- In-Person Solicitations -->
			<xsd:element name="InPersonSolicitationsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>In-person solicitations</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Solicitation of Non-Govt Grants -->
			<xsd:element name="SolicitationOfNonGovtGrantsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Solicitation of non-govt grants</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Solicitation of Governmentt Grants -->
			<xsd:element name="SolicitationOfGovtGrantsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Solicitation of government grants</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Special Fundraising Events -->
			<xsd:element name="SpecialFundraisingEventsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Special fundraising events</Description>
						<LineNumber>Part I Line 1</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Agreement Reference Fundraising? -->
			<xsd:element name="AgrmtProfFundraisingActyInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Did the organization have a written or oral agreement with any individual (including officers, directors, trustees or key employees listed in Form 990, Part VII) or entity in connection with professional fundraising activities?</Description>
						<LineNumber>Part I Line 2a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Fundraiser Activity Information -->
			<xsd:element name="FundraiserActivityInfoGrp" type="FundraiserActivityInfoGrpType" minOccurs="0" maxOccurs="10">
				<xsd:annotation>
					<xsd:documentation>
						<Description>If Line 2a is "Yes," list the ten highest paid individuals or entities (fundraisers) pursuant to agreements under which the fundraiser is to be compensated at least $5,000 by the organization</Description>
						<LineNumber>Part I Line 2b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Gross Receipts -->
			<xsd:element name="TotalGrossReceiptsAmt" type="USAmountNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total gross receipts</Description>
						<LineNumber>Part I Line 2b(iv)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Retained by Contractors -->
			<xsd:element name="TotalRetainedByContractorsAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total retained by contractors</Description>
						<LineNumber>Part I Line 2b(v)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Net to Organization -->
			<xsd:element name="TotalNetToOrganizationAmt" type="USAmountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Total net to organization</Description>
						<LineNumber>Part I Line 2b(vi)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			<!-- Licensed States -->
			<xsd:choice minOccurs="0">
				<xsd:element name="LicensedStatesCd" type="StateType" maxOccurs="60">
					<xsd:annotation>
						<xsd:documentation>
							<Description>List all states in which the organization is registered or licensed to solicit funds or has been notified it is exempt from registration or licensing</Description>
							<LineNumber>Part I Line 3</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="AllStatesCd">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Literal for all states</Description>
							<LineNumber>Part I Line 3</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="StringType">
							<xsd:enumeration value="All States"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:choice>

			<!-- Part II Events -->
			<!-- Events Information -->
			<xsd:element name="FundraisingEventInformationGrp" type="FundraisingEventInformationGrpType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete if the organization answered "Yes" to Form 990, Part IV, line 18, or reported more than $15,000 on Form 990-EZ, line 6a</Description>
						<LineNumber>Part II</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part III  -->
			<!-- Gaming Information -->
			<xsd:element name="GamingInformationGrp" type="GamingInformationGrpType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete this part if the organization reported more than $15,000 on Form 990, Part VIII, line 9a or on Form 990-EZ, line 6a</Description>
						<LineNumber>Part III Lines 1-8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- States Where Gaming Conducted? -->
			<xsd:element name="StatesWhereGamingConductedCd" type="StateType" minOccurs="0" maxOccurs="60">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter state where organization conducts gaming activities</Description>
						<LineNumber>Part III Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Licensed? -->
			<xsd:element name="LicensedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is organization licensed in each state?</Description>
						<LineNumber>Part III Line 9a</LineNumber>
