					<xsd:annotation>
						<xsd:documentation>
							<Description>Person name of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="ThirdPartyBusinessName" type="BusinessNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Business name of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>

			<!-- Address of Third Party -->
			<xsd:choice minOccurs="0">
				<xsd:element name="ThirdPartyUSAddress" type="USAddressType">			
					<xsd:annotation>
						<xsd:documentation>
							<Description>US address of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
