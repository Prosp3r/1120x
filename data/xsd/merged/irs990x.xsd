	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule I ================ -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleI">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule I</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleIType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleI">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule I (Form 990) Supplemental Information on Grants and Other Assistance to Organizations, Governments and Individuals in the U.S. -->

	<xsd:complexType name="IRS990ScheduleIType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule I</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- Part I, General Information on Grants and Assistance -->
			<!-- Records Maintained? -->
			<xsd:element name="GrantRecordsMaintainedInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Does the organization maintain records to substantiate the amount of the grants or assistance, the grantees' eligibility for the grants or assistance, and the selection criteria used to award the grants or assistance?</Description>
						<LineNumber>Part I Line 1</LineNumber> 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part II, Grants and Other Assistance to Governments and Organizations in the United States -->

			<!-- Recipient Table -->
			<xsd:element name="RecipientTable" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Complete if the organization reported more than $5,000 on Form 990, Part IX, line 1 for any recipient that received more than $5,000</Description>
						<LineNumber>Part II</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Name of Recipient -->
						<xsd:element name="RecipientBusinessName" type="BusinessNameType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Name - Business</Description>
									<LineNumber>Part II Line 1 Column (a)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Address of Recipient -->
						<xsd:choice minOccurs="0">
							<xsd:element name="USAddress" type="USAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address of recipient - US</Description>
										<LineNumber>Part II Line  1 Column (a)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="ForeignAddress" type="ForeignAddressType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Address of recipient - Foreign</Description>
										<LineNumber>Part II Line  1 Column (a)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>

						<!-- EIN of Recipient -->
						<xsd:element name="RecipientEIN" type="EINType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>EIN of recipient</Description>
									<LineNumber>Part II Line  1 Column (b)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- IRC Section -->
						<xsd:element name="IRCSectionDesc" type="ShortDescriptionType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>IRC code section if applicable</Description>
									<LineNumber>Part II Line  1 Column (c)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Cash Grant -->
						<xsd:element name="CashGrantAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of cash grant</Description>
									<LineNumber>Part II Line  1 Column (d)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Non-Cash Assistance -->
						<xsd:element name="NonCashAssistanceAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of non-cash assistance</Description>
									<LineNumber>Part II Line  1 Column (e)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Method of Valuation -->
						<xsd:element name="ValuationMethodUsedDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Method of valuation (book. FMV, appraisal, other)</Description>
									<LineNumber>Part II Line  1 Column (f)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Description of Non-Cash Grant -->
						<xsd:element name="NonCashAssistanceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Description of non-cash assistance</Description>
									<LineNumber>Part II Line  1 Column (g)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Purpose of Grant -->
						<xsd:element name="PurposeOfGrantTxt" type="ShortExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Purpose of grant</Description>
									<LineNumber>Part II Line  1 Column (h)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Total Number of 501(C)(3) and Government Grants -->
			<xsd:element name="Total501c3OrgCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter total number of 501(c)(3) and government organizations</Description>
						<LineNumber>Part II Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Total Number of Other Organizations -->
			<xsd:element name="TotalOtherOrgCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Enter total number of other organizations</Description>
						<LineNumber>Part II Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Part III, Grants and Other Assistance to Individuals in the United States -->
			<!-- Grants and Other Assistance Table -->
			<xsd:element name="GrantsOtherAsstToIndivInUSGrp" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part III content</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Type of Grant -->
						<xsd:element name="GrantTypeTxt" type="ShortExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Type of grant or assistance</Description>
									<LineNumber>Part III Column (a)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Number of Recipients -->
						<xsd:element name="RecipientCnt" type="IntegerNNType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Number of recipients</Description>
									<LineNumber>Part III Column (b)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Cash Grant -->
						<xsd:element name="CashGrantAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of cash grant</Description>
									<LineNumber>Part III Column (c)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Amount of Non-Cash Assistance -->
						<xsd:element name="NonCashAssistanceAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Amount of non-cash assistance</Description>
									<LineNumber>Part III Column (d)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Method of Valuation -->
						<xsd:element name="ValuationMethodUsedDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Method of valuation (book, FMV, appraisal, other)</Description>
									<LineNumber>Part III Column (e)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Description of Non-Cash Assistance -->
						<xsd:element name="NonCashAssistanceDesc" type="ShortExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Description of non-cash assistance</Description>
									<LineNumber>Part III Column (f)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

			<!-- Part IV -->
			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Part IV content</Description>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference</Description>
									<LineNumber>Part IV</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Reference Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, part and line number reference explanation</Description>
									<LineNumber>Part IV</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule J ================ -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleJ">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule J</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="IRS990ScheduleJType">
					<xsd:attributeGroup ref="DocumentAttributes">
						<xsd:annotation>
							<xsd:documentation>Common return document attributes</xsd:documentation>
						</xsd:annotation>
					</xsd:attributeGroup>
					<xsd:attribute name="documentName" type="xsd:string" fixed="IRS990ScheduleJ">
						<xsd:annotation>
							<xsd:documentation>IRS internal use only. To avoid error in the return, do not include the attribute name or value.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	
	<!-- Schedule J (Form 990) Supplemental Compensation Information -->
	<!-- For certain Officers, Directors, Trustees, Key Employees, and Highly Compensated Employees -->
	<xsd:complexType name="IRS990ScheduleJType">
		<xsd:annotation>
			<xsd:documentation>Content model for Form 990 Schedule J</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>

			<!-- First Class or Charter Travel -->
			<xsd:element name="FirstClassOrCharterTravelInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>First class or charter travel</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Travel for Companions -->
			<xsd:element name="TravelForCompanionsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Travel for companions</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Idemnification and Gross-Up Payments -->
			<xsd:element name="IdemnificationGrossUpPmtsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Idemnification and gross-up payments</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Discretionary Spending Account -->
			<xsd:element name="DiscretionarySpendingAcctInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Discretionary spending account</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Housing Allowance or Residence -->
			<xsd:element name="HousingAllowanceOrResidenceInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Housing allowance or residence</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Payments For Use of Residence -->
			<xsd:element name="PaymentsForUseOfResidenceInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Payments for use of residence</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Club Dues or Fees -->
			<xsd:element name="ClubDuesOrFeesInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Club dues or fees</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Personal Services -->
			<xsd:element name="PersonalServicesInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Personal services</Description>
						<LineNumber>Part I Line 1a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Written Policy Reference T and E Expenses? -->
			<xsd:element name="WrittenPolicyRefTAndEExpnssInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Written policy reference T and E expenses?</Description>
						<LineNumber>Part I Line 1b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Substantiation Required? -->
			<xsd:element name="SubstantiationRequiredInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Substantiation required?</Description>
						<LineNumber>Part I Line 2</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Committee -->
			<xsd:element name="CompensationCommitteeInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation committee</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Independent Consultant -->
			<xsd:element name="IndependentConsultantInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Independent consultant</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Form 990 of Other Organizations -->
			<xsd:element name="Form990OfOtherOrganizationsInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Form 990 of other organizations</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Written Employment Contract -->
			<xsd:element name="WrittenEmploymentContractInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Written employment contract</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Survey -->
			<xsd:element name="CompensationSurveyInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation survey</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Board or Committee Approval -->
			<xsd:element name="BoardOrCommitteeApprovalInd" type="CheckboxType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Board or committee approval</Description>
						<LineNumber>Part I Line 3</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Severance Payment? -->
			<xsd:element name="SeverancePaymentInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Severance payment?</Description>
						<LineNumber>Part I Line 4a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Supplemental Nonqualified Retirement Plan? -->
			<xsd:element name="SupplementalNonqualRtrPlanInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Supplemental nonqualified retirement plan?</Description>
						<LineNumber>Part I Line 4b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Equity Based Compensation Arrangement? -->
			<xsd:element name="EquityBasedCompArrngmInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Equity based compensation arrangement?</Description>
						<LineNumber>Part I Line 4c</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Based On Revenue of Filing Org? -->
			<xsd:element name="CompBasedOnRevenueOfFlngOrgInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation based on revenue of filing org?</Description>
						<LineNumber>Part I Line 5a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Based On Revenue of Related Orgs? -->
			<xsd:element name="CompBsdOnRevRelatedOrgsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation based on revenue of related orgs?</Description>
						<LineNumber>Part I Line 5b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Based On Net Earnings of Filing Org? -->
			<xsd:element name="CompBsdNetEarnsFlngOrgInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation based on net earnings of filing org?</Description>
						<LineNumber>Part I Line 6a</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Compensation Based On Net Earnings of Related Orgs? -->
			<xsd:element name="CompBsdNetEarnsRltdOrgsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Compensation based on net earnings of related orgs?</Description>
						<LineNumber>Part I Line 6b</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Any Non-Fixed Payments? -->
			<xsd:element name="AnyNonFixedPaymentsInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Any non-fixed payments?</Description>
						<LineNumber>Part I Line 7</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Initial Contract Exception? -->
			<xsd:element name="InitialContractExceptionInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Initial contract exception?</Description>
						<LineNumber>Part I Line 8</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<!-- Rebuttable Presumption Procedure? -->
			<xsd:element name="RebuttablePresumptionProcInd" type="BooleanType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Rebuttable presumption procedure?</Description>
						<LineNumber>Part I Line 9</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="RltdOrgOfficerTrstKeyEmplGrp" minOccurs="0" maxOccurs="unbounded">		
				<xsd:annotation>
					<xsd:documentation>Part II contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>		
					<xsd:sequence>

						<!-- Name of Officer -->
						<xsd:choice minOccurs="0">
							<xsd:element name="PersonNm" type="PersonNameType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Name of officer - person</Description>
										<LineNumber>Part II Column (A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>

							<xsd:element name="BusinessName" type="BusinessNameType">
								<xsd:annotation>
									<xsd:documentation>
										<Description>Name of officer - business</Description>
										<LineNumber>Part II Column (A)</LineNumber>
									</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:choice>

						<!-- Title of Officer -->
						<xsd:element name="TitleTxt" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Title of Officer</Description>
									<LineNumber>Part II Column (A)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Column (B) Breakdown of W-2 or 1099-MISC Compensation -->
						<!-- Base Compensation from Filing Organization -->
						<xsd:element name="BaseCompensationFilingOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Base compensation ($) from filing organization</Description>
									<LineNumber>Part II Column (B)(i)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Compensation Based on Related Organizations? -->
						<xsd:element name="CompensationBasedOnRltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Compensation based on related organizations?</Description>
									<LineNumber>Part II Column (B)(i) </LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Bonus and Incentive Compensation from Filing Organization -->
						<xsd:element name="BonusFilingOrganizationAmount" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Bonus and incentive compensation ($) from filing organization</Description>
									<LineNumber>Part II Column (B)(ii)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Bonus from Related Organizations -->
						<xsd:element name="BonusRelatedOrganizationsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Bonus and incentive compensation ($) from related organizations</Description>
									<LineNumber>Part II Column (B)(ii) </LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Compensation from Filing Organization -->
						<xsd:element name="OtherCompensationFilingOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other compensation ($) from filing organization</Description>
									<LineNumber>Part II Column (B)(iii)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Other Compensation from Related Organizations -->
						<xsd:element name="OtherCompensationRltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Other compensation ($) from related organizations</Description>
									<LineNumber>Part II Column (B)(iii)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Deferred Compensation from Filing Organization -->
						<xsd:element name="DeferredCompensationFlngOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Deferred compensation ($) from filing organization</Description>
									<LineNumber>Part II Column (C)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Deferred Compensation from Related Organizations -->
						<xsd:element name="DeferredCompRltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Deferred compensation ($) from related organizations</Description>
									<LineNumber>Part II Column (C)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Nontaxable Benefits from Filing Organization -->
						<xsd:element name="NontaxableBenefitsFilingOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Nontaxable benefits ($) from filing organization</Description>
									<LineNumber>Part II Column (D)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Nontaxable Benefits from Related Organizations -->
						<xsd:element name="NontaxableBenefitsRltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Nontaxable benefits ($) from related organizations</Description>
									<LineNumber>Part II Column (D)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Total of (B)(i) - (D), From Filing Organizations -->
						<xsd:element name="TotalCompensationFilingOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Total of (B)(i) - (D), from filing org</Description>
									<LineNumber>Part II Column (E)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Total of (B)(i) - (D), From Related Organizations -->
						<xsd:element name="TotalCompensationRltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Total of (B)(i) - (D), from related orgs</Description>
									<LineNumber>Part II Column (E)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Comp Reported Prior 990 - From Filing Organization -->
						<xsd:element name="CompReportPrior990FilingOrgAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Comp reported prior 990 - from filing org</Description>
									<LineNumber>Part II Column (F)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Comp Reported Prior 990 - From Related Organizations -->
						<xsd:element name="CompReportPrior990RltdOrgsAmt" type="USAmountType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Comp reported prior 990 - from related orgs</Description>
									<LineNumber>Part II Column (F)</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>	
				</xsd:complexType>		
			</xsd:element>		

			<xsd:element name="SupplementalInformationDetail" minOccurs="0" maxOccurs="10">		
				<xsd:annotation>
					<xsd:documentation>Part III contents</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>		
					<xsd:sequence>

						<!-- Form, Part and Line Number Reference -->
						<xsd:element name="FormAndLineReferenceDesc" type="LineExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, Part and line number reference</Description>
									<LineNumber>Part III</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

						<!-- Form, Part and Line Number Reference Explanation -->
						<xsd:element name="ExplanationTxt" type="ExplanationType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>
									<Description>Form, Part and line number reference explanation</Description>
									<LineNumber>Part III</LineNumber>
								</xsd:documentation>
							</xsd:annotation>
						</xsd:element>

					</xsd:sequence>	
				</xsd:complexType>		
			</xsd:element>		

		</xsd:sequence>
	</xsd:complexType>
	
	<!-- ====================================================================== -->
	<!-- ==================  IRS Form 990 Schedule K ========================== -->
	<!-- ====================================================================== -->
	
	<xsd:element name="IRS990ScheduleK">
		<xsd:annotation>
			<xsd:documentation>IRS Form 990 Schedule K</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
