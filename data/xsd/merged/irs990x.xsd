						<Description>Current tax year minus one year</Description>
						<LineNumber>Part II and III Column (d)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Current Tax Year -->
			<xsd:element name="CurrentTaxYearAmt" type="USAmountType" minOccurs="0">
