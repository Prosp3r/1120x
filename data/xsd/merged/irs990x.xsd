				<xsd:element name="ThirdPartyForeignAddress" type="ForeignAddressType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Foreign address of third party</Description>
							<LineNumber>Part III Line 15c</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
	
			<!-- Gaming Manager Name -->
			<xsd:choice minOccurs="0">
				<xsd:element name="GamingManagerPersonNm" type="PersonNameType">
					<xsd:annotation>
						<xsd:documentation>
							<Description>Gaming manager person name</Description>
							<LineNumber>Part III Line 16</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>

				<xsd:element name="GamingManagerBusinessName" type="BusinessNameType">
					<xsd:annotation>
