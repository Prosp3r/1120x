					<xsd:annotation>
						<xsd:documentation>
							<Description>Contributor address - US</Description>
							<LineNumber>Part I Column (b)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
					
				<xsd:element name="ContributorForeignAddress" type="ForeignAddressType">
