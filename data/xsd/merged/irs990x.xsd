	</xsd:complexType>
	<!-- Type for Repeating Group in Part I, Line 11g - Information About the Supported Organizations -->
	<xsd:complexType name="SupportedOrgInformationGrpType">
		<xsd:sequence>
			<!-- Name -->
			<xsd:element name="SupportedOrganizationName" type="BusinessNameType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Name</Description>
						<LineNumber>Part I Line 11g Column (i)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- EIN of Supported Organization -->
			<xsd:element name="EIN" type="EINType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>EIN of supported organization</Description>
						<LineNumber>Part I Line 11g Column (ii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Type of Organization -->
			<xsd:element name="OrganizationTypeCd"  minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Type of organization</Description>
						<LineNumber>Part I Line 11g(iii)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="StringType">
					    <xsd:enumeration value="1">
						<xsd:annotation>
							<xsd:documentation>Church</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="2">
						<xsd:annotation>
							<xsd:documentation>School</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="3">
						<xsd:annotation>
							 <xsd:documentation>Hospital</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="4">
						<xsd:annotation>
							 <xsd:documentation>Medical research organization</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="5">
						<xsd:annotation>
							 <xsd:documentation>College or university</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="6">
						<xsd:annotation>
							 <xsd:documentation>Federal, state or local government</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="7">
						<xsd:annotation>
							 <xsd:documentation>Governmental unit or general public</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="8">
						<xsd:annotation>
							 <xsd:documentation>Community trust</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					    <xsd:enumeration value="9">
						<xsd:annotation>
							 <xsd:documentation>More than 33 1/3 support</xsd:documentation>
						</xsd:annotation>
					    </xsd:enumeration>
					</xsd:restriction>
				    </xsd:simpleType>
			</xsd:element>
			<!-- Is the Supported Organization Listed in Your Governing Documents -->
			<xsd:element name="GoverningDocumentListedInd" type="BooleanType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Is the supported organization listed in your governing documents?</Description>
						<LineNumber>Part I Line 11g Column (iv)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Amount of Support -->
			<xsd:element name="SupportAmt" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Amount of support</Description>
						<LineNumber>Part I Line 11g Column (v)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!-- Estimated Value of Diversion -->
			<xsd:element name="OtherSupportAmt" type="USAmountType" minOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Estimated value of diversion</Description>
						<LineNumber>Part I Line 11g Column (vi)</LineNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- Type for Part II and III, Columns (a) through (f) -->
	<xsd:complexType name="Form990SchAPartIIGroup1Type">
