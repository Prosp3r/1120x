						<xsd:documentation>
							<Description>Pd. 527(j)(1)</Description>
							<LineNumber>Part I Column (b)</LineNumber>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>				
		  </xsd:choice>			

			<!-- Contributor Address -->
