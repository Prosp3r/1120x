	<!-- Type for Part I, Columns (a) through (f) Table -->
	<xsd:complexType name="Form990SchHPartIGroup1Type">
		<xsd:sequence>

			<!-- Number of Activities or Programs -->
			<xsd:element name="ActivitiesOrProgramsCnt" type="IntegerNNType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
